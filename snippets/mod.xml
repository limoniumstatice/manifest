<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote name="gitlab"
          fetch="https://gitlab.com"
          sync-c="true" />

  <!-- remove-project -->
  <remove-project name="LineageOS/android_packages_apps_Etar" />
  <remove-project name="LineageOS/android_packages_apps_Seedvault" />
  <remove-project name="LineageOS/android_packages_apps_Updater" />
  <remove-project name="LineageOS/android_packages_screensavers_PhotoTable" />
  <remove-project name="LineageOS/android_vendor_lineage" />
  <remove-project name="platform/external/noto-fonts" />
  <remove-project name="platform/packages/screensavers/Basic" />

  <!-- remove Darwin -->
  <remove-project name="platform/prebuilts/bazel/darwin-x86_64" />
  <remove-project name="platform/prebuilts/clang/host/darwin-x86" />
  <remove-project name="platform/prebuilts/go/darwin-x86" />

  <!-- remove Infrastructure -->
  <remove-project name="LineageOS/charter" />
  <remove-project name="LineageOS/cm_crowdin" />
  <remove-project name="LineageOS/hudson" />
  <remove-project name="LineageOS/mirror" />
  <remove-project name="LineageOS/www" />
  <remove-project name="LineageOS/lineage_wiki" />

  <project path="external/noto-fonts" name="limoniumstatice/noto-fonts" groups="pdk" remote="github" />
  <project path="vendor/extra" name="limoniumstatice/vendor_extra" remote="gitlab" />
  <project path="vendor/lineage" name="limoniumstatice/vendor_floko" remote="github" />
</manifest>
